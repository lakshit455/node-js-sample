pipeline {
    agent any
    
    }

    stages{
        stage("npm install"){
            steps{
                sh ' npm install '
            }
            
            }
              stage("starting server"){
            steps{
                sh ' node index.js '
            }
            
            }
               
              
              
        }
        
    post{
        always{
            echo "========always========"
        }
        success{
            echo "========pipeline executed successfully ========"
        }
        failure{
            echo "========pipeline execution failed========"
        }
    }
  }
}
